<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.tnovoselec.lifesumfoodsearch"
          xmlns:android="http://schemas.android.com/apk/res/android">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme.NoActionBar"
    android:name=".LifesumFoodSearchApplication">
    <activity
      android:name=".activity.FoodListActivity"
      android:label="@string/saved_items"
      >
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".activity.FoodSearchActivity"/>
    <activity android:name=".activity.FoodDetailsActivity"/>
  </application>

</manifest>
